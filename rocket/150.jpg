//gobacktop
$(function(){
    //當滾動條的位置處於距頂部100像素以下時，跳轉鏈結出現，否則消失
    $(function () {
        $(window).scroll(function(){
            if ($(window).scrollTop()>100){
                $(".backtop").fadeIn(500);
            }
            else
            {
                $(".backtop").fadeOut(500);
            }
        });
        //當點擊跳轉鏈結後，回到頁面頂部位置
        $(".backtop").click(function(){
            $('body,html').animate({scrollTop:0},800);
            return false;
        });
    });
});
